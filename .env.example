# Environment Variables for Monitoring and Analytics
# Copy this file to .env and fill in your actual values

# Sentry Configuration
# Get your DSN from Sentry.io -> Project Settings -> Client Keys (DSN)
VITE_SENTRY_DSN=https://your-dsn@sentry.io/project-id

# Google Analytics 4 Configuration
# Get your Measurement ID from Google Analytics -> Admin -> Data Streams -> Web Stream Details
VITE_GA4_MEASUREMENT_ID=G-XXXXXXXXXX

# Application Version
# Useful for tracking errors and performance by version
VITE_APP_VERSION=1.0.0

# Existing Environment Variables (keep these)
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
VITE_AIRTABLE_PAT=your-airtable-pat
VITE_AIRTABLE_BASE_ID=your-airtable-base-id

# Development Override (for testing)
# Set to 'true' to enable monitoring in development
VITE_ENABLE_MONITORING_IN_DEV=false

# Optional: Custom API Keys
VITE_GEMINI_API_KEY=your-gemini-api-key